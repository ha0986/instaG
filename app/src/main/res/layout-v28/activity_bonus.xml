                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"

                android:id="@+id/cardView6"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                app:cardBackgroundColor="#1B1E28"
                app:cardCornerRadius="20dp">

                <Button
